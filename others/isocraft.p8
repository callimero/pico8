-- transition --
function trans_init()
	music(-1)
	trans={
		o=true,
		p=0,
		t=0,
		mt=20,
		quit=false,
	}
	-- on,pixels,time,middle time
end

function trans_update()
	if trans.t<trans.mt then
		trans.p+=256/trans.mt
	elseif trans.t>trans.mt then
		trans.p-=256/trans.mt
		if trans.t>trans.mt*2 then
			trans.o=false
		end
	else
		if trans.quit==true then
			title_init()
		else
			game_init()
		end
	end
	trans.t+=1
end

function trans_draw()
	local c=0
	local x=0 local y=0
	while c<trans.p do
		pal(2, ceil(rnd(15)))
		spr(16, x, y)
		x+=8
		if x>120 then
			y+=8
			x=0
		end
		c+=1
	end
	pal(2, 2)
end
