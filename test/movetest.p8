pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
function _init()
 sp={x=24,y=10,sx=1,sy=1}
end

function _update()
	sp.x+=sp.sx
	sp.y+=sp.sy
	if sp.x>=120 or sp.x<=0 then sp.sx*=-1 end
	if sp.y>=120 or sp.y<=0 then sp.sy*=-1 end
end

function _draw()
	cls()
	rect(0,0,127,127)
	rect(1,1,126,126)
	spr(2,flr(sp.x),flr(sp.y))
end

