pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
function _init()
t=0
lr=0
px=64
py=64
end

function _update60()
if btn() then
	t=(t+1)%10 
end
if btnp(➡️) then
	lr=0
	dx=false
	dy=false
	px=px+8
end
if btnp(⬅️) then
	lr=0
	dx=true
	dy=false
	px=px-8
end
if btnp(⬆️) then
	lr=2
	dy=false
	py=py-8
end
if btnp(⬇️) then
	lr=2
	dy=true
	py=py+8
end


end

function _draw()
	cls()
	map()
	spr(25,px,py,1,1,dx,dy)
	spr(1+lr+t/5,px,py,1,1,dx,dy)
	mset(px/8,py/8,24)
end




