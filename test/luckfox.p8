function _init()
	beam={x=10,y=10}
	tx=5.0
	ty=5.0
	sx=0.0051
	sy=0.0051
end

function _draw()
cls()
local c=3
local xa=63+sin(tx/10)*60
local ya=63+cos(ty/10)*60
for i=1,6000 do
	if i<3000 then c=3
	else c=11 end
	tx+=sx
	ty+=sy
	beam.x=63+sin(tx/10)*60
 beam.y=63+cos(ty/10)*60
 line(xa,ya,beam.x,beam.y,c)
 xa=beam.x
 ya=beam.y
 end
end

function _update()
	if btn(⬆️) then
		sx=sx+0.00002
	end
	if btn(⬇️) then
		sx=sx-0.00002
	end
	if btn(⬅️) then
		sy=sy+0.00002
	end
	if btn(➡️) then
		sy=sy-0.00002
	end
-- spr(4,beam.x,beam.y)
-- sfx(0)
end

